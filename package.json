{
  "name": "orange-radio-cms-static",
  "version": "0.0.1",
  "description": "Static screens for Orange Radio CMS using boosted",
  "scripts": {
    "dev": "parcel ./src/index.html",
    "prebuild": "npx rimraf ./docs",
    "build": "parcel build . src/index.html --public-url $PUBLIC_URL --no-content-hash --dist-dir docs"
  },
  "targets": {
    "browser": {
      "source": "src/index.html",
      "distDir": "docs"
    }
  },
  "dependencies": {
    "@popperjs/core": "^2.9.3",
    "boosted": "^5.1.3",
    "flag-icons": "^6.1.1",
    "sortablejs": "^1.15.0"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "^2.8.3",
    "cssnano": "^5.0.7",
    "parcel": "^2.8.3",
    "postcss": "^8.3.6",
    "posthtml-expressions": "^1.9.0",
    "posthtml-include": "^1.7.4",
    "sass": "^1.37.5"
  },
  "author": "Chris Masters",
  "license": "ISC"
}
